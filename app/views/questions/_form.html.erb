<%= render "layouts/errors", :object => @question %>

<div class="card">
  <%= form_for @question do |f| %>
    <%= f.label :question%>
    <%= f.text_field :question %>
    <%= f.label :category%>
    <%= f.text_field :category %>
    <%= f.hidden_field :user_id, :value => current_user.id %>
    <%= f.submit 'Submit', :class => "btn-dark" %>
  <% end %>
</div>
